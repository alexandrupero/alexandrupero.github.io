{
  "name": "alexandrupero.github.io",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "pnpm lint:js && pnpm lint:style && pnpm lint:prettier",
    "lintfix": "prettier --write --list-different . && pnpm lint:js --fix && pnpm lint:style --fix",
    "prepare": "husky install",
    "postinstall": "nuxt prepare",
    "typecheck": "tsc"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.6",
    "daisyui": "^2.50.0",
    "nuxt": "^3.2.0",
    "pinia": "^2.0.30",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@nuxt/devtools": "^0.1.3",
    "@nuxt/image-edge": "1.0.0-27919678.2f5b64b",
    "@nuxt/types": "^2.16.0",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/tailwindcss": "^6.4.0",
    "@types/node": "^18.13.0",
    "autoprefixer": "^10.4.13",
    "core-js": "^3.27.2",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint": "^0.8.19",
    "lint-staged": "^13.1.1",
    "nuxt-icon": "^0.3.2",
    "nuxt-vite": "^0.3.5",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.0",
    "stylelint": "^15.1.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^30.0.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-stylelint": "^4.2.0",
    "vue-server-renderer": "^2.7.14",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.75.0"
  }
}
