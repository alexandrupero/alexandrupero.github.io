    "@types/node": "^22.10.5",
    "autoprefixer": "^10.4.20",
    "core-js": "^3.39.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint": "^0.8.19",
    "lint-staged": "^15.3.0",
    "nuxt-icon": "1.0.0-beta.7",
    "nuxt-security": "^2.1.5",
    "nuxt-vite": "^0.3.5",
    "postcss": "^8.4.49",
