    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/image-edge": "1.3.0-28493895.369b747",
    "@nuxt/types": "^2.18.1",
